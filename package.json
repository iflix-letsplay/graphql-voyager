    "json-loader": "^0.5.7",
    "postcss-cssnext": "^3.0.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "raw-loader": "^0.5.1",
    "react": "^15.6.1",
    "react-clipboard.js": "^1.1.2",
